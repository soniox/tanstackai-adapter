{
  "name": "@soniox/tanstack-ai-adapter",
  "version": "0.1.1",
  "description": "Soniox transcription adapter for TanStack AI",
  "author": "Soniox Inc.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soniox/tanstackai-adapter.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/soniox/tanstackai-adapter",
  "bugs": "https://github.com/soniox/tanstackai-adapter/issues",
  "type": "module",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "vite build",
    "clean": "rm -rf dist *.tsbuildinfo",
    "test:eslint": "eslint ./src",
    "test:lib": "vitest --passWithNoTests",
    "test:types": "tsc",
    "test:live": "node tests/live-transcription.mjs",
    "verdaccio": "verdaccio --config .verdaccio/config.yaml",
    "test:publish": "npm run build && npm publish --registry http://localhost:4873 --force"
  },
  "keywords": [
    "ai",
    "soniox",
    "tanstack",
    "adapter",
    "transcription"
  ],
  "peerDependencies": {
    "@tanstack/ai": "^0.2.2"
  },
  "devDependencies": {
    "@tanstack/ai": "^0.2.2",
    "@tanstack/vite-config": "^0.1.0",
    "@types/node": "^20.19.0",
    "@vitest/coverage-v8": "^4.0.16",
    "typescript": "^5.6.3",
    "verdaccio": "^6.0.0",
    "vite": "^7.2.7",
    "vitest": "^4.0.16"
  }
}
